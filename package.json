{
  "name": "ramverk2",
  "version": "6.0.0",
  "description": "Me-page with express and react",
  "main": "react-frontend/index.js",
  "scripts": {
    "start": "node express-backend/src/server.js",
    "start-client": "cd react-frontend && npm start",
    "start-backend": "node express-backend/src/server.js",
    "start-dev": "concurrently --kill-others \"npm run start-client\" \"npm run start-backend\"",
    "install": "cd react-frontend && npm install && npm run build && cd ../express-backend && npm install",
    "test": "cd react-frontend && npm test",
    "test-cov": "cd react-frontend && npm test -- --coverage",
    "posttest": "make test",
    "test1": "docker-compose run node_9_test",
    "test2": "docker-compose run node_7_test",
    "test3": "docker-compose run node_8_test",
    "start-docker": "docker-compose up -d mongodb",
    "reset-database": "node express-backend/src/mongodb/setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcusgsta/ramverk2.git"
  },
  "author": "Marcus Gustafsson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcusgsta/ramverk2/issues"
  },
  "homepage": "https://github.com/marcusgsta/ramverk2#readme",
  "devDependencies": {
    "ajv": "5.0.3-beta.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-preset-env": "^1.6.1",
    "clean-css-cli": "^4.1.10",
    "codecov": "^3.0.0",
    "coveralls": "^3.0.0",
    "csslint": "^1.0.5",
    "eslint": "^4.12.1",
    "eslint-config-postcss": "^2.0.2",
    "eslint-plugin-pug": "^1.1.1",
    "eslint-plugin-react": "^7.5.1",
    "gulp-postcss": "^7.0.0",
    "html-minifier": "^3.5.7",
    "htmlhint": "^0.9.13",
    "js-yaml": "^3.10.0",
    "jscs": "^3.0.7",
    "jsonlint": "^1.6.2",
    "mocha": "^4.0.1",
    "nyc": "^11.4.0",
    "postcss-reporter": "^5.0.0",
    "postcss-scss": "^1.0.2",
    "stylelint": "^8.3.1",
    "stylelint-config-recommended-scss": "^2.0.0",
    "stylelint-scss": "^2.2.0",
    "concurrently": "^3.5.1"
  }
}
